// examplenumber 7 (Symbol)

let exampleSymbol = Symbol("symbols are primitive data type, symbols are often used to create private or unique object properties you can create symbols by using the symbol() function.");
let obj = {
    [exampleSymbol]: 'value'
};
console.log(obj[exampleSymbol])

